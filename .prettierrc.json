{
  "printWidth": 80,

  "tabWidth": 2,

  "useTabs": false,

  "semi": true,

  "singleQuote": true,

  "jsxSingleQuote": true,

  "singleAttributePerLine": true,

  "trailingComma": "es5",

  "bracketSpacing": true,

  "jsxBracketSameLine": false,

  "arrowParens": "always",

  "endOfLine": "lf",

  "plugins": ["prettier-plugin-tailwindcss"]
}
